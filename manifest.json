{
    "name": "AWS Internal DNS Resolver",
    "version": "1.0",
    "description": "Resolve your Internal DNS Names to Public DNS Names magically",
    "manifest_version": 2,
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "permissions": [
      "webRequest",
      "<all_urls>",
      "webRequestBlocking"
    ]
}
